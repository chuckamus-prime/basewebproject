{
	"compilerOptions": {
		"module": "commonjs",
		"target": "ES5",
		"moduleResolution": "node",
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"noImplicitAny": false,
		"removeComments": true,
		"sourceMap": true
	},
	"exclude": [
		"./node_modules",
		"./.idea",
		"./target"
	]
}